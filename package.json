  "dependencies": {},
  "devDependencies": {
    "@release-it/conventional-changelog": "^10.0.3",
    "@types/react": "^18.0.25",
    "@types/react-native": "^0.72.2",
    "expo-module-scripts": "^3.0.11",
    "expo-modules-core": "^1.5.6",
    "release-it": "^19.1.0"
  },
  "peerDependencies": {
